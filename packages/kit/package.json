{
  "name": "@wahab/kit",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "gen:svg": "node ./scripts/svg-iconpack.js",
    "dev": "yarn run storybook",
    "build": "rimraf dist/ && yarn build:lib",
    "build:lib": "tsc --outDir dist/",
    "lint": "yarn run lint:es && yarn run lint:styling",
    "lint:es": "eslint --fix src/**/*.tsx",
    "lint:styling": "stylelint --fix src/components/**/*.tsx",
    "precommit": "yarn test",
    "tsck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": ".*\\.test\\.(ts|tsx)$"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.22",
    "@storybook/cli": "^7.0.22",
    "@storybook/nextjs": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/react-webpack5": "^7.0.22",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/color": "^3.0.3",
    "@types/node": "^18.16.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "bootstrap": "4.6.2",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "storybook": "^7.0.22",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-loader": "^9.4.2"
  },
  "dependencies": {
    "@rjsf/core": "^5.7.3",
    "@rjsf/utils": "^5.7.3",
    "@rjsf/validator-ajv8": "^5.7.3",
    "@sinclair/typebox": "^0.28.14",
    "clean-react-props": "^0.4.0",
    "color": "^4.2.3",
    "fs": "^0.0.1-security",
    "react-bootstrap": "^2.7.4",
    "react-jsonschema-form": "^1.8.1",
    "style-object-to-css-string": "^1.0.1",
    "styled-components": "^5.3.10"
  }
}
