{
  "name": "@wahab/kit",
  "version": "0.1.0",
  "license": "MIT",
  "main": "src/",
  "types": "src/",
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "gen:svg": "node ./scripts/svg-iconpack.js",
    "dev": "yarn run storybook",
    "build": "rimraf dist/ && yarn compile",
    "compile": "tsc",
    "lint": "yarn run lint:es && yarn run lint:styling",
    "lint:es": "eslint --fix src/**/*.tsx",
    "lint:styling": "stylelint --fix src/components/**/*.tsx",
    "precommit": "yarn test",
    "tsck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": ".*\\.test\\.(ts|tsx)$"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/react": "^7.4.6",
    "@types/color": "^3.0.4",
    "@types/lodash": "^4.14.199",
    "@types/lodash.castarray": "^4.4.7",
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isarray": "^4.0.7",
    "@types/lodash.keys": "^4.2.7",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.mapvalues": "^4.6.7",
    "@types/lodash.reduce": "^4.6.7",
    "@types/lodash.set": "^4.3.7",
    "@types/styled-components": "^5.1.28",
    "husky": "^8.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "storybook": "^7.0.22",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-loader": "^9.4.2"
  },
  "dependencies": {
    "clean-react-props": "^0.4.0",
    "color": "^4.2.3",
    "fs": "^0.0.1-security",
    "lodash.castarray": "^4.4.0",
    "lodash.chunk": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isarray": "^4.0.0",
    "lodash.keys": "^4.2.0",
    "lodash.map": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.set": "^4.3.2",
    "react-bootstrap": "^2.7.4",
    "style-object-to-css-string": "^1.0.1",
    "styled-components": "^5.3.10"
  }
}
